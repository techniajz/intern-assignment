<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title><%= siteTitle %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="shortcut icon" href="<%= siteUrl %>/accessory.svg">
    <link href="<%= siteUrl %>/libs/flatpickr/flatpickr.min.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/bootstrap-colorpicker/bootstrap-colorpicker.min.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/clockpicker/bootstrap-clockpicker.min.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/bootstrap-datepicker/bootstrap-datepicker.min.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/datatables/dataTables.bootstrap4.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/datatables/responsive.bootstrap4.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/datatables/buttons.bootstrap4.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/datatables/select.bootstrap4.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/sweetalert2/sweetalert2.min.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/jquery-nice-select/nice-select.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/select2/select2.min.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/bootstrap-select/bootstrap-select.min.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/bootstrap-touchspin/jquery.bootstrap-touchspin.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/css/icons.min.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/css/app.min.css" rel="stylesheet" type="text/css">
    <script src="<%= siteUrl %>/js/jquery.min.js"></script>
</head>
<body>
<div id="wrapper">
    <% include ../layouts/partials/topBar %>
    <% include ../layouts/partials/leftSideBar %>
    <div class="content-page">
        <div class="content">
            <div class="container-fluid">
                <% if (errorFlash || successFlash || infoFlash) { %>
                    <div class="row">
                        <div class="col-md-12" style="margin-top: 15px">
                            <% if (errorFlash){ %>
                                <div class="alert alert-danger alert-dismissible fade show" role="alert">
                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                        <span aria-hidden="true">×</span>
                                    </button>
                                    <%= errorFlash %>
                                </div>
                            <% } if (successFlash){ %>
                                <div class="alert alert-success alert-dismissible fade show" role="alert">
                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                        <span aria-hidden="true">×</span>
                                    </button>
                                    <%= successFlash %>
                                </div>
                            <% } if (infoFlash){ %>
                                    <div class="alert alert-info alert-dismissible fade show" role="alert">
                                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                            <span aria-hidden="true">×</span>
                                        </button>
                                        <%= infoFlash %>
                                    </div>
                            <% } %>
                        </div>
                    </div>
                <% } %>
                <%- body %>
            </div>
        </div>
        <% include ../layouts/partials/footer %>
    </div>
</div>
<!-- <script src="<%= siteUrl %>/js/vendor.min.js"></script> -->
<script src="<%= siteUrl %>/libs/datatables/jquery.dataTables.min.js"></script>
<script src="<%= siteUrl %>/libs/datatables/dataTables.bootstrap4.js"></script>
<script src="<%= siteUrl %>/libs/datatables/dataTables.responsive.min.js"></script>
<script src="<%= siteUrl %>/libs/datatables/responsive.bootstrap4.min.js"></script>
<script src="<%= siteUrl %>/libs/datatables/dataTables.buttons.min.js"></script>
<script src="<%= siteUrl %>/libs/datatables/buttons.bootstrap4.min.js"></script>
<script src="<%= siteUrl %>/libs/datatables/buttons.html5.min.js"></script>
<script src="<%= siteUrl %>/libs/datatables/buttons.flash.min.js"></script>
<script src="<%= siteUrl %>/libs/datatables/buttons.print.min.js"></script>
<script src="<%= siteUrl %>/libs/datatables/dataTables.keyTable.min.js"></script>
<script src="<%= siteUrl %>/libs/datatables/dataTables.select.min.js"></script>
<script src="<%= siteUrl %>/libs/pdfmake/pdfmake.min.js"></script>
<script src="<%= siteUrl %>/libs/pdfmake/vfs_fonts.js"></script>
<script src="<%= siteUrl %>/js/pages/datatables.js"></script>
<script src="<%= siteUrl %>/libs/sweetalert2/sweetalert2.min.js"></script>
<script src="<%= siteUrl %>/libs/select2/select2.min.js"></script>
<script src="<%= siteUrl %>/libs/flatpickr/flatpickr.min.js"></script>
<script src="<%= siteUrl %>/libs/jquery-nice-select/jquery.nice-select.min.js"></script>
<script src="<%= siteUrl %>/libs/bootstrap-select/bootstrap-select.min.js"></script>
<script src="<%= siteUrl %>/libs/bootstrap-touchspin/jquery.bootstrap-touchspin.min.js"></script>
<script src="<%= siteUrl %>/libs/bootstrap-datepicker/bootstrap-datepicker.min.js"></script>
<script src="<%= siteUrl %>/libs/jquery-knob/jquery.knob.min.js"></script>
<script src="<%= siteUrl %>/libs/jquery-sparkline/jquery.sparkline.min.js"></script>
<script src="<%= siteUrl %>/js/app.min.js"></script>
<script src="<%= siteUrl %>/js/delivery-drop.js"></script>

</body>
</html>